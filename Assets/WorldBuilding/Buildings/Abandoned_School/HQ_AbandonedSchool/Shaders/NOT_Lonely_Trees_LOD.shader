				float4 ase_color : COLOR;
				UNITY_VERTEX_INPUT_INSTANCE_ID
				UNITY_VERTEX_OUTPUT_STEREO
			};

			CBUFFER_START(UnityPerMaterial)
			float4 _BaseColor_ST;
			float4 _Color01;
			float4 _Color02;
			float4 _MetallicRAOGGlossA_ST;
			float4 _Normal_ST;
			float _MaskContrast;
